{
  "name": "se-test-assignment",
  "version": "1.0.0",
  "description": "Assignment for Software Engineering School",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint --ext .js,.ts src",
    "prettier": "prettier src --write",
    "prettier:check": "prettier src --check ",
    "dotenv": "node ./node_modules/dotenv/lib/main.js",
    "build": "tsc",
    "start": "ts-node ./dist",
    "dev:start": "npm-run-all build start",
    "dev": "dotenv -- nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "typeorm": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
    "typeorm:run": "dotenv -- npm run typeorm migration:run -- -d ./src/ormconfig.ts",
    "typeorm:revert": "dotenv -- npm run typeorm migration:revert -- -d ./src/ormconfig.ts",
    "test": "jest --setupFiles dotenv/config"
  },
  "pre-commit": [
    "eslint",
    "prettier:check",
    "test"
  ],
  "keywords": [],
  "author": "Arina Kysilova",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jest": "^29.7.0",
    "joi": "^17.4.1",
    "k6": "^0.0.0",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.13",
    "pg": "^8.7.1",
    "pre-commit": "^1.2.2",
    "sinon": "^18.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "typeorm": "^0.3.20",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.12",
    "@types/k6": "^0.51.0",
    "@types/node": "^16.4.1",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.15",
    "@types/safe-timers": "^1.1.2",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  }
}
